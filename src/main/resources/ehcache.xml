<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" updateCheck="false"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" name="hymanCacheMgr"
         maxBytesLocalDisk="50G">
    <defaultCache
            maxElementsInMemory="10000"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            maxElementsOnDisk="10000000"
            diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LRU">
    </defaultCache>

    <cache name="wsCache" timeToLiveSeconds="900" maxBytesLocalHeap="50M"/>
</ehcache>